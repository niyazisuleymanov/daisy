{
  "name": "incog",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "permissions": ["storage", "tabs"],
  "incognito": "spanning",
	"content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "app.js"
      ],
      "run_at": "document_start"
    }
  ]
}
